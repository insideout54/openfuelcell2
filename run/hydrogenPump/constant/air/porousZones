FoamFile
{
    version         2.0;
    format          ascii;
    class           dictionary;
    object          porousZones;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

cathode
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        cathode;		// name of porous zone
    porosity        0.7;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e9 1e9 1e9);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
        rho thermo:rho;
    }

    Cp 568;
    k  1;
}

mpl
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        mpl;		// name of porous zone
    porosity        0.4;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e12 1e12 1e12);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
        rho thermo:rho;
    }

    Cp    568;
    k     1;
}

ccl
{
    type            DarcyForchheimer;	// porosity Model: fixCoeff, powerLaw, DarcyForchheimer
    active          yes;		// status
    cellZone        ccl;		// name of porous zone
    porosity        0.3;		// porosity

    DarcyForchheimerCoeffs		// coefficents for DarcyForchheimer; with the name of type + Coeffs
    {
        d   (1e13 1e13 1e13);
        f   (0 0 0);

        coordinateSystem
        {
            type    cartesian;
            origin  (0 0 0);
            coordinateRotation
            {
                type    axesRotation;
                e1      (1 0 0);    //(0.70710678 0.70710678 0);
                e2      (0 0 1);
            }
        }
        rho thermo:rho;
    }

    Cp        568;
    k         1;
}
