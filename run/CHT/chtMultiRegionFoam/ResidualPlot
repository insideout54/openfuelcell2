set logscale y
set title "/*---------------------------------------------------------------------------*\"
set ylabel 'Residual'
set xlabel 'Iteration'
plot \
"< cat log.run | grep 'Solving for p_rgh' | cut -d' ' -f9 | tr -d ','" every 6 title 'p' with lines,\
"< cat log.run | grep 'Solving for h' | cut -d' ' -f9 | tr -d ','" every 2 title 'h' with lines,\
"< cat log.run | grep 'Solving for e' | cut -d' ' -f9 | tr -d ','" every 1 title 'e' with lines,\

pause 1
reread
